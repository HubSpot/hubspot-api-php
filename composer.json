{
    "name": "ibrhaim13/api-client",
    "version": "9.1",
    "description": "This fork of (Hubspot API client/official package) to be compatible with php 7.1 or above with some customize",
    "keywords": [
        "hubspot",
        "swagger",
        "php",
        "sdk",
        "api",
        "ibrhaim13",
        "ibrahim"
    ],
    "homepage": "https://github.com/ibrhaim13/hubspot-api-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ibrahim Musabeh",
            "homepage": "https://github.com/ibrhaim13/hubspot-api-php"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.5",
        "phpspec/phpspec": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "HubSpot\\Client\\" : "codegen/",
            "HubSpot\\" : "lib/",
            "Hubspot\\Tests\\": "tests/"
        }
    }
}
