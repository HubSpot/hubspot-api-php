{
    "name": "hubspot/api-client",
    "version": "3.1.0",
    "description": "Hubspot API client",
    "keywords": [
        "hubspot",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://hubspot.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "HubSpot",
            "homepage": "https://hubspot.com/"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 | ^7.0",
        "guzzlehttp/psr7": "1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.12",
        "phpunit/phpunit": "^8.5",
        "phpspec/phpspec": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "HubSpot\\Client\\" : "codegen/",
            "HubSpot\\" : "lib/",
            "Hubspot\\Tests\\": "tests/"
        }
    }
}
